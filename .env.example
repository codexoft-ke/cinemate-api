# Environment variables
DEBUG=True
SECRET_KEY=django-insecure-example-secret-key
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0,example.com

# Database configuration
DB_ENGINE=django.db.backends.postgresql
DB_NAME=example_db
DB_USER=example_user
DB_PASSWORD=example_password
DB_HOST=localhost
DB_PORT=5432

# Redis configuration
REDIS_URL=redis://:example_redis_password@localhost:6379/1
CACHE_TTL=300

# TMDb API configuration
TMDB_ACCESS_TOKEN=example_tmdb_access_token
TMDB_BASE_URL=https://api.themoviedb.org/3

# JWT configuration
JWT_SECRET_KEY=example_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_LIFETIME=3600
JWT_REFRESH_TOKEN_LIFETIME=604800

# Email configuration
EMAIL_BACKEND=django.core.mail.backends.console.EmailBackend
EMAIL_HOST=example.smtp.com
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=example_user@example.com
EMAIL_HOST_PASSWORD=example_email_password

# Rate limiting
RATE_LIMIT_ENABLE=True

# CORS configuration
CORS_ALLOW_ALL_ORIGINS=True